CREATE TABLE `finance_test`. ( `id` INT NOT NULL AUTO_INCREMENT , `task` VARCHAR(250) NOT NULL , `assigned_to` VARCHAR(50) NOT NULL , `completed` BOOLEAN NOT NULL DEFAULT FALSE , PRIMARY KEY (`id`)) ENGINE = InnoDB;

